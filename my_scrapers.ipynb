        "#@title Helper functions\n",
        "\n",
        "def get_website(website_link, cookie_accept = \"NONE\"):\n",
        "  chrome.get(website_link)\n",
        "  time.sleep(3)\n",
        "  \n",
        "  # Accept cookies\n",
        "  try:\n",
        "    chrome.find_element(By.XPATH, cookie_accept).click()\n",
        "  except:\n",
        "    chrome.delete_all_cookies()\n",
        "  \n",
        "def get_pdfs(xpath_ref, names):\n",
        "  # Find all hrefs containing pdf in the given website \n",
        "  pdfs = chrome.find_elements(By.XPATH, \"//a[contains(@href, {})]\".format(xpath_ref))\n",
        "  href = {}\n",
        "\n",
        "  for i in range(min(len(names), len(pdfs))): \n",
        "    link = pdfs[i].get_attribute(\"href\")\n",
        "    #href.update({names[i].text:link})\n",
        "    href.update({names[i]:link})\n",
        "\n",
        "  return href\n",
        "  \n",
        "def download_pdfs(dir_name, fund_name, href):\n",
        "  for i in href: \n",
        "    data = requests.get(href[i])\n",
        "    if not os.path.exists(dir_name):\n",
        "      os.mkdir(dir_name)\n",
        "    fl = dir_name\n",
        "    with open(os.path.join(fl,i + ' ' + fund_name+ \".pdf\"), 'wb') as download:\n",
        "      download.write(data.content)"
